    "version": "0.2.0",
    "configurations": [
        {
            "name": "Cli create roles",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build-cli",
            "program": "${workspaceFolder}/src/Tlis.Cms.Cli/src/bin/Debug/net8.0/Tlis.Cms.Cli.dll",
            "args": ["create-roles"],
            "cwd": "${workspaceFolder}/src/Tlis.Cms.Cli/src",
            "stopAtEntry": false,
            "env": {
                "ASPNETCORE_ENVIRONMENT": "Development"
            },
            "sourceFileMap": {
                "/Views": "${workspaceFolder}/Views"
            }
        }
    ]
}